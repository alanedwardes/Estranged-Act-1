//"unlitgeneric"
//"SDK_LightmappedGeneric"
"sdk_unlittwotexture"
{
	"$basetexture" "overlays/shorewave001b"
	"$texture2"    "overlays/shorewave001b"
	"$translucent" 1
//	"$additive" 1
	"$decal" 1
//	"$ignorez" 1

	$vertexalpha 1

//  VARS

	"$ScrollRateResult"	1


	"Proxies"
	{
		"Sine"
		{
			"resultVar"	"$ScrollRateResult"
			"sineperiod"	13
			"sinemin"	.0002
			"sinemax"	.002
		}

		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" .1
			"texturescrollangle" 90
		}

		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" .01
			"texturescrollangle" 90
		}


}

}
